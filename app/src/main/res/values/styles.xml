<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="MyButtonPrimary" parent="Widget.Material3.Button">
        <item name="backgroundTint">@color/blue</item>
        <item name="android:textColor">@color/white</item>
        <item name="cornerRadius">10dp</item>
        <item name="android:padding">20dp</item>
        <item name="android:textSize">20dp</item>
        <item name="fontFamily">@font/plus_jakarta_sans_extrabold</item>
    </style>
    <style name="MyButton" parent="Widget.Material3.Button">
        <item name="backgroundTint">@color/blur</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:padding">20dp</item>
        <item name="cornerRadius">10dp</item>
        <item name="android:textSize">20dp</item>
        <item name="fontFamily">@font/plus_jakarta_sans_extrabold</item>
    </style>
    <style name="ButtonBack" parent="Widget.Material3.Button">
        <item name="backgroundTint">@color/blur</item>
        <item name="android:textColor">@color/white</item>
        <item name="cornerRadius">100dp</item>
        <item name="android:padding">0dp</item>
    </style>
    <style name="Title" parent="Widget.Material3.Button">
        <item name="fontFamily">@font/plus_jakarta_sans_extrabold</item>
        <item name="color">@color/white</item>
        <item name="android:textStyle">bold</item>
        <item name="android:padding">0dp</item>
    </style>
    <style name="Body" parent="Widget.Material3.Button">
        <item name="fontFamily">@font/plus_jakarta_sans</item>
        <item name="color">@color/white</item>
        <item name="android:padding">0dp</item>
    </style>
</resources>